{
  "index": "/index.html?ref=pwa",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/assets/*.css",
          "/*.js",
          "/assets/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "API",
      "urls": [
        "https://api.shopper.ink/v1/**",
        "!https://api.shopper.ink/v1/bcds",
        "!https://api.shopper.ink/v1/bcds/*"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "14d",
        "timeout": "5s",
        "strategy": "freshness"
      }
    }
  ]
}
